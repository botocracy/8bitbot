{
  "name": "8bitbot",
  "version": "1.0.0",
  "description": "8BitBot",
  "repository": {
    "type": "git",
    "url": "git://github.com/botocracy/8bitbot.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.1.0"
  },
  "main": "./src/index.js",
  "browser": "./src/index.js",
  "keywords": [
    "bittorrent",
    "hls",
    "hlsjs",
    "mse",
    "p2p",
    "peer-to-peer",
    "torrent",
    "video",
    "webrtc",
    "webtorrent"
  ],
  "scripts": {
    "audit": "bash tools/npm-scripts.sh audit",
    "build": "bash tools/npm-scripts.sh build",
    "clean": "bash tools/npm-scripts.sh clean",
    "compile": "bash tools/npm-scripts.sh compile",
    "depends": "bash tools/npm-scripts.sh depends",
    "depends-build": "bash tools/npm-scripts.sh depends-build",
    "depends-checkout": "bash tools/npm-scripts.sh depends-checkout",
    "depends-install": "bash tools/npm-scripts.sh depends-install",
    "format": "bash tools/npm-scripts.sh format",
    "lint": "bash tools/npm-scripts.sh lint",
    "postinstall": "bash tools/npm-scripts.sh postinstall",
    "preinstall": "bash tools/npm-scripts.sh preinstall",
    "start": "bash tools/npm-scripts.sh start",
    "test": "bash tools/npm-scripts.sh test"
  },
  "config": {
    "publicInterfaceContracts": "IOwnable,LibOwnableRichErrors,LibRichErrors,Ownable",
    "abis:comment": "This list is auto-generated by contracts-gen. Don't edit manually.",
    "abis": "./test/generated-artifacts/@(IOwnable|LibOwnableRichErrors|LibRichErrors|Ownable|TestOwnable).json"
  },
  "dependencies": {
    "0x.js": "^9.2.0",
    "@0x/connect": "^6.0.9",
    "@0x/contract-wrappers": "^13.9.5",
    "@0x/contracts-erc721": "^3.1.13",
    "@0x/mesh-browser": "^10.1.0",
    "@0x/migrations": "^6.4.7",
    "@0x/order-utils": "^10.4.5",
    "@0x/subproviders": "^6.1.9",
    "@0x/utils": "^6.1.0",
    "@0x/web3-wrapper": "^7.2.8",
    "cache-chunk-store": "^3.0.0",
    "dexie": "^3.0.2",
    "hls.js": "^0.13.2",
    "jsonld": "^3.2.0",
    "mediasource": "^2.3.0",
    "p2p-media-loader-core": "^0.6.2",
    "p2p-media-loader-hlsjs": "^0.6.2",
    "path": "^0.12.7",
    "simple-peer": "git+https://github.com/feross/simple-peer.git#master",
    "three": "^0.121.1",
    "video.js": "^7.8.4",
    "web3-provider-engine": "^16.0.1",
    "webtorrent": "^0.108.6"
  },
  "devDependencies": {
    "@0x/abi-gen": "^5.4.8",
    "@0x/contracts-gen": "^2.0.19",
    "@0x/sol-compiler": "^4.3.0",
    "@0x/typescript-typings": "^5.1.5",
    "@ethereum-waffle/chai": "^3.2.0",
    "@ethereum-waffle/compiler": "^3.2.0",
    "@ethereum-waffle/ens": "^3.2.0",
    "@ethereum-waffle/mock-contract": "^3.2.0",
    "@ethereum-waffle/provider": "^3.2.0",
    "@openzeppelin/contracts": "^3.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@snowpack/plugin-typescript": "^1.0.2",
    "@snowpack/plugin-webpack": "^2.0.17",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^5.2.7",
    "@types/sinon-chai": "^3.2.5",
    "@types/video.js": "^7.3.11",
    "@videojs/vhs-utils": "^2.2.1",
    "audit-ci": "^3.1.1",
    "bip39": "^2.6.0",
    "buffer": "^5.7.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.6.5",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "esm": "^3.2.25",
    "eth-block-tracker": "^4.4.3",
    "eth-sig-util": "^2.5.3",
    "ethereum-waffle": "^3.2.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "^4.0.48",
    "hdkey": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom-global": "^3.0.2",
    "ltgt": "^2.2.1",
    "mocha": "^8.2.0",
    "npm-force-resolutions": ">=0.0.3",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-pkg": "^0.8.0",
    "prettier-plugin-sh": "^0.6.0",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "readable-stream": "^3.6.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "snowpack": "^2.15.1",
    "threads": "^1.6.3",
    "threads-plugin": "^1.3.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.5"
  },
  "resolutions": {
    "@0x/subproviders": "6.1.9",
    "bip39": "2.6.0",
    "buffer": "5.7.1",
    "eth-block-tracker": "4.4.3",
    "eth-sig-util": "2.5.3",
    "ethereumjs-abi": "0.6.8",
    "ltgt": "2.2.1",
    "readable-stream": "3.6.0"
  },
  "bundleDependencies": false,
  "chromeapp": {
    "fs-chunk-store": "memory-chunk-store",
    "http": "http-node",
    "load-ip-set": false,
    "net": "chrome-net",
    "os": false
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "import/no-unresolved": 2,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-console": 1,
      "max-len": 0,
      "prefer-template": 2,
      "no-use-before-define": 0,
      "newline-per-chained-call": 0,
      "arrow-body-style": [
        2,
        "as-needed"
      ]
    }
  },
  "target": "web"
}
