{
  "name": "8bitbot",
  "version": "1.0.0",
  "description": "8BitBot",
  "repository": {
    "type": "git",
    "url": "git://github.com/botocracy/8bitbot.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.1.0"
  },
  "main": "./src/index.js",
  "browser": "./src/index.js",
  "keywords": [
    "bittorrent",
    "hls",
    "hlsjs",
    "mse",
    "p2p",
    "peer-to-peer",
    "torrent",
    "video",
    "webrtc",
    "webtorrent"
  ],
  "scripts": {
    "audit": "bash tools/npm-install.sh audit",
    "build": "bash tools/npm-tasks.sh build",
    "clean": "bash tools/npm-tasks.sh clean",
    "depends": "bash tools/npm-depends.sh all",
    "depends:build": "bash tools/npm-depends.sh build",
    "depends:checkout": "bash tools/npm-depends.sh checkout",
    "depends:install": "bash tools/npm-depends.sh install",
    "format": "bash tools/npm-tasks.sh format",
    "lint": "bash tools/npm-tasks.sh lint",
    "postinstall": "bash tools/npm-install.sh postinstall",
    "start": "bash tools/npm-tasks.sh start",
    "test": "bash tools/npm-tasks.sh test"
  },
  "dependencies": {
    "hls.js": "^0.14.16",
    "jsonld": "^3.2.0",
    "three": "^0.121.1",
    "webtorrent": "^0.108.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@snowpack/plugin-webpack": "^2.1.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "audit-ci": "^3.1.1",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "isomorphic-fetch": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-pkg": "^0.8.0",
    "prettier-plugin-sh": "^0.6.0",
    "prettier-plugin-solidity": "^1.0.0-alpha.60",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "snowpack": "^2.17.1",
    "threads": "^1.6.3",
    "threads-plugin": "^1.3.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.5"
  },
  "bundleDependencies": false,
  "chromeapp": {
    "fs-chunk-store": "memory-chunk-store",
    "http": "http-node",
    "load-ip-set": false,
    "net": "chrome-net",
    "os": false
  },
  "target": "web"
}
