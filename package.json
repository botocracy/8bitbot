{
  "name": "8bitbot",
  "version": "1.0.0",
  "description": "8BitBot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/botocracy/8bitbot.git"
  },
  "keywords": [
    "bittorrent",
    "hls",
    "hlsjs",
    "mse",
    "p2p",
    "peer-to-peer",
    "torrent",
    "video",
    "webrtc",
    "webtorrent"
  ],
  "target": "web",
  "bundleDependencies": false,
  "main": "./src/index.js",
  "browser": "./src/index.js",
  "scripts": {
    "postinstall": "bash tools/npm-scripts.sh postinstall",
    "start": "bash tools/npm-scripts.sh start",
    "depends": "bash tools/npm-scripts.sh depends",
    "depends-checkout": "bash tools/npm-scripts.sh depends-checkout",
    "depends-build": "bash tools/npm-scripts.sh depends-build",
    "depends-install": "bash tools/npm-scripts.sh depends-install",
    "build": "bash tools/npm-scripts.sh build",
    "audit": "bash tools/npm-scripts.sh audit",
    "lint": "bash tools/npm-scripts.sh lint",
    "test": "bash tools/npm-scripts.sh test",
    "format": "bash tools/npm-scripts.sh format",
    "clean": "bash tools/npm-scripts.sh clean"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@snowpack/plugin-typescript": "^1.0.2",
    "@snowpack/plugin-webpack": "^2.0.17",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "audit-ci": "^3.1.1",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "isomorphic-fetch": "^2.2.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.2.0",
    "prettier": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "snowpack": "^2.15.1",
    "threads": "^1.6.3",
    "threads-plugin": "^1.3.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "hls.js": "^0.13.2",
    "jsonld": "^3.2.0",
    "three": "^0.121.1",
    "webtorrent": "^0.108.6"
  },
  "chromeapp": {
    "fs-chunk-store": "memory-chunk-store",
    "http": "http-node",
    "load-ip-set": false,
    "net": "chrome-net",
    "os": false
  }
